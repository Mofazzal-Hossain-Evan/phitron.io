//5-5 Capital or Small or Digit

char x;
scanf("%c", &x);
if(x>='0' && x<='9')
{
printf("IS DIGIT\n");
}
else
{
    printf("ALPHA\n");
    if(x>='a' && x<='z')
    {
        printf("IS SMALL\n");
    } 
    else
    {
        printf("IS CAPITAL\n");
    }
}

    return 0;
}